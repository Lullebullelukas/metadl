P("tests/evaluation/metadl-java/src/boxed-primitive-constructor/").

TypeName("Character"), TypeName("Integer"), TypeName("Long"), TypeName("Short"),
TypeName("Byte"), TypeName("Float"), TypeName("Double"), TypeName("Boolean").

