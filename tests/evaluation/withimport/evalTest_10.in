IMPORT("tests/evaluation/withimport/evalTest_10_input.mdl", 'Program).

# Extract predicates referred in the deprecated relation

analyze('Program) {
 Deprecated(p) :- <: $d($p) . :>, ID($d, "Deprecated"), REF($p, p).
 Warning(p, l, c) :- <: ... :- ..., $p(...), ... . :>,
                  ID($p, p), Deprecated(p), SRC($p, l, c).

 Warning(p, l, c) :- <: ..., $p(...), ... :- ... . :>,
                  ID($p, p), Deprecated(p), SRC($p, l, c).

 Warning(p, l, c) :- <: ..., $p(...), ... . :>,
                  ID($p, p), Deprecated(p), SRC($p, l, c).
}


OUTPUT('Warning).
